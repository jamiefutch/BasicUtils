<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Authors>Jamie Futch</Authors>
    <Description>Basic utils for C#</Description>
    <Copyright>Jamie Futch, 2023</Copyright>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/jamiefutch/BasicUtils</PackageProjectUrl>
    <Version>1.0.8</Version>
    <PackageVersion>1.0.17</PackageVersion>
    <AssemblyVersion>1.0.17</AssemblyVersion>
    <FileVersion>1.0.17</FileVersion>
    <RepositoryUrl>https://github.com/jamiefutch/BasicUtils</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>console utils; string utils</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageLicenseUrl></PackageLicenseUrl>
    <TargetFrameworks>netstandard2.0;net6.0;net7.0</TargetFrameworks>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="6.0.0" />
  </ItemGroup>

</Project>
